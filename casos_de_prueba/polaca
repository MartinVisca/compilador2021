program
    LONG y;
    LONG a;
    LONG b;
    LONG FUNC f(LONG x)
        LONG FUNC f1(LONG z)
        LONG x;
        BEGIN
            x := 1;
            RETURN(x);
        END;
    BEGIN
        x := f1(a);
        RETURN(x);
    END;
    BEGIN
        a := 2;
        b := 3;
        y := f(a);
    END;